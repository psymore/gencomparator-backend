generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DB_URL")
}

model user {
    id        String   @id @default(uuid())
    email     String
    createdAt DateTime
}

model api_keys {
    id        String   @id @default(uuid())
    name      String
    user_id   Int
    key       String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
